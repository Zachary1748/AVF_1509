if(Ti.Network.online)require("ui");else{alert("You're not connected to a network.");var db=require("db")}!function(){var e=require("ti.cloud"),o="production"===Ti.App.deployType.toLowerCase()?"production":"development",n=Ti.App.Properties.getString("acs-username-"+o),i=Ti.App.Properties.getString("acs-password-"+o);o&&n&&i&&e.Users.login({login:n,password:i},function(e){"development"===o&&(Ti.API.info("ACS Login Results for environment `"+o+"`:"),Ti.API.info(e)),e&&e.success&&e.users&&e.users.length?Ti.App.fireEvent("login.success",e.users[0],o):Ti.App.fireEvent("login.failed",e,o)})}();