var globalOrientation=[Ti.UI.PORTRAIT,Ti.UI.LANDSCAPE_LEFT,Ti.UI.LANDSCAPE_RIGHT],imageFull=Ti.UI.createWindow({backgroundColor:"333",title:"Full Image",orientationModes:globalOrientation}),showFullImage=function(e){var i=Ti.UI.createImageView({image:e.fullImage,left:20,right:20});imageFull.add(i),navWin.openWindow(imageFull,{animate:!0})};exports.imageFull=imageFull,exports.showFullImage=showFullImage;