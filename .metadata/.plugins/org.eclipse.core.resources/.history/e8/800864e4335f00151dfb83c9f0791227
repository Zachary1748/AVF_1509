var pWidth=Ti.Platform.displayCaps.platformWidth,pHeight=Ti.Platform.displayCaps.platformHeight,rowCount=4,margin=5,size=(pWidth-margin*(rowCount+1))/rowCount,globalOrientation=[Ti.UI.PORTRAIT,Ti.UI.LANDSCAPE_LEFT,Ti.UI.LANDSCAPE_RIGHT],imageWin=Ti.UI.createWindow({title:"Search Results",backButtonTitle:"Back",backgroundColor:"#333333"}),viewContainer=Ti.UI.createScrollView({width:pWidth,height:pHeight-70,contentWidth:pWidth,layout:"horizontal",showVerticalIndicator:!0}),buildUi=function(e){for(var i=0;i<e.length;i++){var a=Ti.UI.createView({width:size,height:size,left:margin,top:margin,borderRadius:10}),t=Ti.UI.createImageView({image:e[i].preview,title:"Image",top:0,width:2*size});a.add(t),viewContainer.add(a)}};imageWin.add(viewContainer),exports.imageWin=imageWin;