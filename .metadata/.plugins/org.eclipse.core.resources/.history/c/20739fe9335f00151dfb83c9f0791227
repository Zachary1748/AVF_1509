var apiConnection=function(e){Ti.API.info("apiConnection");var n="https://pixabay.com/api/?username=Zachary1748&key=552c904285618cf4cf20&q="+encodeURIComponent(e)+"&image_type=photo",o=Ti.Network.createHTTPClient({onload:function(){var e=JSON.parse(this.responseText),n=e.hits,o=require("db");o.save(n)},onerror:function(e){console.log(e),alert("There was an error while retreiving the weather data.")},timeout:5e3});o.open("GET",n),o.send()};exports.apiConnection=apiConnection;