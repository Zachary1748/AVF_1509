var read=function(){Ti.API.info("Read Function!!!!!!!!");for(var e=Ti.Database.open("image_db"),i=e.execute("SELECT * FROM image_data"),o=[];i.isValidRow();){var a={id:i.fieldByName("id"),preview:i.fieldByName("preview"),fullImage:i.fieldByName("fullImage")};o.push(a),i.next()}i.close(),e.close();var n=require("imageView");n.buildUi(o)},save=function(e){Ti.API.info("Save Function!!!!!!!!");var o=Ti.Database.open("image_db");for(o.execute("CREATE TABLE IF NOT EXISTS image_data (id INTEGER PRIMARY KEY, preview TEXT, fullImage TEXT)"),o.execute("DELETE FROM image_data"),i=0;i<e.length;i++)o.execute("INSERT INTO image_data (preview, fullImage) VALUES (?, ?)",e[i].previewURL,e[i].webformatURL);o.close(),read()};exports.read=read,exports.save=save;