var mainWin=Ti.UI.createWindow({title:"Project 3",backgroundColor:"#F0F0F0",layout:"vertical"}),navWin=Titanium.UI.iOS.createNavigationWindow({window:mainWin}),mainView=Ti.UI.createScrollView({top:20,right:0,bottom:0,left:0,layout:"vertical",contentHeight:"100%",contentWidth:"100%"}),searchLabel=Ti.UI.createLabel({text:"Search Images",top:"20%",font:{fontSize:"20%",fontFamily:"Arial",fontWeight:"Bold"}});mainView.add(searchLabel);var searchField=Ti.UI.createTextField({left:20,right:20,top:50,height:40,borderColor:"#EDEDED",backgroundColor:"#FFFFFF",borderRadius:3,hintText:"Jeff Dunham",paddingLeft:10,paddingRight:10});mainView.add(searchField);var searchBtn=Ti.UI.createView({top:5,left:20,right:20,height:40,borderRadius:3,backgroundColor:"#333333"}),searchTxt=Ti.UI.createLabel({text:"Search",color:"#FFFFFF",top:8,font:{fontSize:18,fontFamily:"Arial"}});searchBtn.add(searchTxt),mainView.add(searchBtn),searchBtn.addEventListener("click",function(){var e=searchField.value;if(""==e||null==e)alert("The search field must not be empty.");else{var i=require("imageView");i.viewContainer&&i.imageWin.remove(i.viewContainer);var a=require("api");a.apiConnection(e),navWin.openWindow(i.imageWin,{animated:!0})}}),mainWin.add(mainView),navWin.open();